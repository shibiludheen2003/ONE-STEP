    function increament(id,price){
        const qtyValue = document.getElementById(`quantiy_${id}`);
        const totalValue = document.getElementById(`totalPrice_${id}`);
        if(qtyValue.innerHTML<=9){
    
        fetch(`plus?q=${id}`)
        .then((res)=>{
            return res.json()
        })
        .then((data)=>{
            if(data.changed==true){
                qtyValue.innerText = parseInt(qtyValue.innerText)+1;
                totalValue.innerHTML = parseInt(price*qtyValue.innerText) 

            }
        })
        .catch((err)=>{
           location.href = '/'
        })
    }else{
        Swal.fire({
        title: 'Are you sure?',
          text: `This is out off limit${name}`,
          icon: 'warning',
          showCancelButton: true,
          confirmButtonColor: '#3085d6',
          cancelButtonColor: '#d33',
        })
    
    }
    }
    
    function decreament(id,price,deleId){
        const qtyValue = document.getElementById(`quantiy_${id}`);
        const totalValue = document.getElementById(`totalPrice_${id}`);
        console.log(qtyValue.innerText)
     if(qtyValue.innerText>1){
        fetch(`dic?q=${id}`)
        .then((res)=>{
            return res.json()
        })
        .then((data)=>{
            if(data.changed==true){
                qtyValue.innerText = parseInt(qtyValue.innerText)-1;
                totalValue.innerHTML = parseInt(price*qtyValue.innerText) 
                               
            }
        })
        .catch((err)=>{
           location.href = '/'
        })
    }else{
     Swal.fire({
        title: 'Are you sure?',
          text: `You want to remove the ${name}`,
          icon: 'warning',
          showCancelButton: true,
          confirmButtonColor: '#3085d6',
          cancelButtonColor: '#d33',
          confirmButtonText: 'Yes, remove it!'})
          .then((data)=>{
        
            if(data.isConfirmed){

                
                window.location.href=`deleteCart?q=${deleId}`


            }
          })
          
        
    }
    
    }
