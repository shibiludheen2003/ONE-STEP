    function increament(id,price){
        const qtyValue = document.getElementById(`quantiy_${id}`);
        const totalValue = document.getElementById(`totalPrice_${id}`);
    
        fetch(`plus?q=${id}`)
        .then((res)=>{
            return res.json()
        })
        .then((data)=>{
            if(data.changed==true){
                qtyValue.innerText = parseInt(qtyValue.innerText)+1;
                totalValue.innerHTML = parseInt(price*qtyValue.innerText) 

            }
        })
        .catch((err)=>{
           location.href = '/'
        })
    }
    
    function decreament(id,price,deleId){
